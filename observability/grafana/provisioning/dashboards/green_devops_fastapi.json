{
  "id": null,
  "uid": "green-devops-fastapi",
  "title": "Green DevOps â€” FastAPI",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "stat",
      "title": "Requests / 5s",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 6 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m]))",
          "legendFormat": "rps",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency (p50 / p95)",
      "gridPos": { "x": 6, "y": 0, "w": 18, "h": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(http_request_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_latency_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests In Flight",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "http_requests_in_flight",
          "legendFormat": "inflight",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests by Status (rate)",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 7 },
      "targets": [
        {
          "expr": "sum by (status) (rate(http_requests_total[5m]))",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ]
    }
  ]
}
